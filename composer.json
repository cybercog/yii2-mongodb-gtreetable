{
    "name": "songlipeng2003/yii2-mongodb-gtreetable",
    "description": "yii2-mongodb-gtreetable is an extension of Yii 2 Framework, which is wrapper for bootstrap-gtreetable plug-in, on the other hand it provides functionality which allows to save the nodes states into database.",
    "keywords": ["yii2", "extension", "gtreetable", "tree", "nested"],
    "homepage": "https://github.com/songlipeng2003/yii2-mongodb-gtreetable",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Thinking Song",
            "email": "songlipeng2003@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",   
        "yiisoft/yii2-jui": "*",
        "creocoder/yii2-nested-sets": "0.9.0",
        "yiisoft/yii2-mongodb": "*",
        "bower-asset/bootstrap-gtreetable" : "2.*",
        "bower-asset/jquery.browser": "*",
        "bower-asset/URIjs": "1.14.*"      
    },
    "autoload": {
        "psr-4": {
            "songlipeng2003\\gtreetable\\": ""
        }
    }
}
